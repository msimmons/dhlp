<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>

    <ACTION NAME="dhlp-reload">
        <CODE>
           net.contrapt.dhlp.jedit.DHLPlugin.getInstance().reinit(view);
        </CODE>
    </ACTION>

    <ACTION NAME="dhlp-sql">
        <CODE>
           net.contrapt.dhlp.jedit.DHLPlugin.getInstance().execSql(view);
        </CODE>
    </ACTION>

    <ACTION NAME="dhlp-plan">
        <CODE>
           net.contrapt.dhlp.jedit.DHLPlugin.getInstance().generateQueryPlan(view);
        </CODE>
    </ACTION>

    <ACTION NAME="dhlp-find">
       <CODE>
          net.contrapt.dhlp.jedit.DHLPlugin.getInstance().findObjects(view);
       </CODE>
    </ACTION>

    <ACTION NAME="dhlp-describe">
        <CODE>
           net.contrapt.dhlp.jedit.DHLPlugin.getInstance().describeObject(view);
        </CODE>
    </ACTION>

</ACTIONS>
